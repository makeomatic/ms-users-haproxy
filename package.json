{
  "name": "haproxy",
  "version": "0.0.0",
  "description": "Provides JWT token verification script for HAProxy.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mdep test run",
    "lint": "eslint ./src ./test",
    "semantic-release": "semantic-release",
    "docker-release-haproxy": "mdep docker release -p ms-users-haproxy -f Dockerfile.haproxy",
    "docker-build-haproxy": "mdep docker build -p ms-users-haproxy -f Dockerfile.haproxy",
    "docker-release-token-server": "mdep docker build -p ms-users-token-server -f Dockerfile.token-server",
    "docker-build-token-server": "mdep docker build -p ms-users-token-server -f Dockerfile.token-server",
    "docker-release-haproxy-ingress": "mdep docker release -p 'ms-users-haproxy-ingress' -f Dockerfile.k8s-ingress",
    "docker-build-haproxy-ingress": "mdep docker build --project='ms-users-haproxy-ingress' -f Dockerfile.k8s-ingress",
    "start": "node index.js"
  },
  "dependencies": {
    "@microfleet/core": "^17.15.0",
    "@microfleet/plugin-consul": "^2.11.0",
    "@microfleet/plugin-logger": "^0.10.14",
    "@microfleet/plugin-redis-cluster": "^0.5.14",
    "@microfleet/plugin-redis-core": "^0.5.14",
    "@microfleet/plugin-redis-sentinel": "^0.5.14",
    "@microfleet/plugin-validator": "^0.5.14",
    "@microfleet/validation": "^11.1.2",
    "consul": "^0.40.0",
    "ioredis": "^4.28.5",
    "fastify": "^3.27.4",
    "fastify-plugin": "^3.0.1",
    "lodash": "^4.17.21",
    "ms-conf": "^7.0.2",
    "ms-users": "github:makeomatic/ms-users#feat/refresh-tokens"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/register": "^7.17.0",
    "@makeomatic/deploy": "^12.6.3",
    "axios": "^0.26.0",
    "bluebird": "^3.7.2",
    "commitlint": "^16.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-makeomatic": "^5.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.2.0"
  }
}
